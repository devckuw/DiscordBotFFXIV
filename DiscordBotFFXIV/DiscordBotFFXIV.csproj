<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Dalamud.Plugin.Bootstrap.targets" />

  <PropertyGroup>
    <Version>0.1.4.1</Version>
    <Description>A sample plugin.</Description>
    <PackageProjectUrl>https://github.com/goatcorp/SamplePlugin</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\Data\*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="3.15.3" />
    <PackageReference Include="NetCord" Version="1.0.0-alpha.349" />
    <PackageReference Include="NetCord.Hosting" Version="1.0.0-alpha.349" />
    <PackageReference Include="NetCord.Hosting.AspNetCore" Version="1.0.0-alpha.349" />
    <PackageReference Include="NetCord.Hosting.Services" Version="1.0.0-alpha.349" />
    <PackageReference Include="NetCord.Services" Version="1.0.0-alpha.349" />
  </ItemGroup>
</Project>
